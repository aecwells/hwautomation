{
  "name": "hwautomation-frontend",
  "version": "1.0.0",
  "description": "Frontend build system for HWAutomation web interface",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "watch": "vite build --watch",
    "lint": "eslint src/hwautomation/web/frontend/js --ext .js,.ts",
    "lint:fix": "eslint src/hwautomation/web/frontend/js --ext .js,.ts --fix",
    "format": "prettier --write \"src/hwautomation/web/frontend/**/*.{js,ts,scss,css}\"",
    "format:check": "prettier --check \"src/hwautomation/web/frontend/**/*.{js,ts,scss,css}\"",
    "clean": "rimraf src/hwautomation/web/static/dist"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.15",
    "cssnano": "^6.0.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "sass": "^1.64.1",
    "terser": "^5.43.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7"
  },
  "dependencies": {
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
